{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "measureProcess",
    "sample",
    "plan",
    "method",
    "assay",
    "batch",
    "delivery",
    "personRegistry"
  ],
  "properties": {
    "measureProcess": {
      "type": "object",
      "required": [
        "ttlUrn",
        "identifier",
        "analyst",
        "taring",
        "weighing"
      ],
      "properties": {
        "ttlUrn": {
          "type": "string",
          "faker": "random.uuid"
        },
        "identifier": {
          "type": "string",
          "faker": "random.uuid"
        },
        "analyst": {
          "type": "object",
          "required": [
            "ttlUrn",
            "name"
          ],
          "properties": {
            "ttlUrn": {
              "type": "string",
              "faker": "random.uuid"
            },
            "name": {
              "type": "string",
              "examples": [
                "jxd00004"
              ],
              "pattern": "^jxd[0-9]{5}$"
            }
          }
        },
        "taring": {
          "type": "object",
          "required": [
            "ttlUrn",
            "modelId",
            "identifier",
            "endDatum",
            "plan",
            "results"
          ],
          "properties": {
            "ttlUrn": {
              "type": "string",
              "faker": "random.uuid"
            },
            "modelId": {
              "type": "string",
              "examples": [
                "AFP_0000640"
              ],
              "pattern": "^AFP_[0-9]{7}$"
            },
            "identifier": {
              "type": "string",
              "faker": "random.uuid"
            },
            "endDatum": {
              "type": "string",
              "format": "date-time",
              "faker": "customDate.last7"
            },
            "plan": {
              "type": "object",
              "required": [
                "ttlUrn"
              ],
              "properties": {
                "ttlUrn": {
                  "type": "string",
                  "faker": "random.uuid"
                }
              }
            },
            "results": {
              "type": "object",
              "required": [
                "weight",
                "deviation"
              ],
              "properties": {
                "weight": {
                  "type": "object",
                  "required": [
                    "value",
                    "unit"
                  ],
                  "properties": {
                    "value": {
                      "type": "number",
                      "chance": {
                        "floating": {
                          "min": 25,
                          "max": 1000,
                          "fixed": 2
                        }
                      }
                    },
                    "unit": {
                      "type": "string",
                      "enum": [
                        "Milligram"
                      ]
                    }
                  }
                },
                "deviation": {
                  "type": "object",
                  "required": [
                    "value",
                    "unit"
                  ],
                  "properties": {
                    "value": {
                      "type": "number",
                      "chance": {
                        "floating": {
                          "min": 0.01,
                          "max": 1,
                          "fixed": 2
                        }
                      }
                    },
                    "unit": {
                      "type": "string",
                      "enum": [
                        "Milligram"
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "weighing": {
          "type": "object",
          "required": [
            "ttlUrn",
            "identifier",
            "endDatum",
            "plan",
            "results"
          ],
          "properties": {
            "ttlUrn": {
              "type": "string",
              "faker": "random.uuid"
            },
            "identifier": {
              "type": "string",
              "faker": "random.uuid"
            },
            "endDatum": {
              "type": "string",
              "format": "date-time",
              "faker": "customDate.last7"
            },
            "plan": {
              "type": "object",
              "required": [
                "ttlUrn"
              ],
              "properties": {
                "ttlUrn": {
                  "type": "string",
                  "faker": "random.uuid"
                }
              }
            },
            "results": {
              "type": "object",
              "required": [
                "ttlUrn",
                "grossWeigth",
                "grossDeviation",
                "netWeigth",
                "netDeviation"
              ],
              "properties": {
                "ttlUrn": {
                  "type": "string",
                  "faker": "random.uuid"
                },
                "grossWeigth": {
                  "type": "object",
                  "required": [
                    "value",
                    "unit"
                  ],
                  "properties": {
                    "value": {
                      "type": "number",
                      "chance": {
                        "floating": {
                          "min": 50,
                          "max": 1000,
                          "fixed": 2
                        }
                      }
                    },
                    "unit": {
                      "type": "string",
                      "enum": [
                        "Milligram"
                      ]
                    }
                  }
                },
                "grossDeviation": {
                  "type": "object",
                  "required": [
                    "value",
                    "unit"
                  ],
                  "properties": {
                    "value": {
                      "type": "number",
                      "chance": {
                        "floating": {
                          "min": 0.01,
                          "max": 1,
                          "fixed": 2
                        }
                      }
                    },
                    "unit": {
                      "type": "string",
                      "enum": [
                        "Milligram"
                      ]
                    }
                  }
                },
                "netWeigth": {
                  "type": "object",
                  "required": [
                    "value",
                    "unit"
                  ],
                  "properties": {
                    "value": {
                      "type": "number",
                      "chance": {
                        "floating": {
                          "min": 50,
                          "max": 1000,
                          "fixed": 2
                        }
                      }
                    },
                    "unit": {
                      "type": "string",
                      "enum": [
                        "Milligram"
                      ]
                    }
                  }
                },
                "netDeviation": {
                  "type": "object",
                  "required": [
                    "value",
                    "unit"
                  ],
                  "properties": {
                    "value": {
                      "type": "number",
                      "chance": {
                        "floating": {
                          "min": 0.01,
                          "max": 1,
                          "fixed": 2
                        }
                      }
                    },
                    "unit": {
                      "type": "string",
                      "enum": [
                        "Milligram"
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "sample": {
      "type": "object",
      "required": [
        "container"
      ],
      "properties": {
        "container": {
          "type": "object",
          "required": [
            "ttlUrn",
            "modelId",
            "name",
            "material",
            "custodian"
          ],
          "properties": {
            "ttlUrn": {
              "type": "string",
              "faker": "random.uuid"
            },
            "modelId": {
              "type": "string",
              "examples": [
                "AFE_0000407"
              ],
              "pattern": "^AFE_[0-9]{7}$"
            },
            "name": {
              "type": "string",
              "examples": [
                "BAR0000001"
              ],
              "pattern": "^BAR[0-9]{7}$"
            },
            "material": {
              "type": "object",
              "required": [
                "ttlUrn",
                "modelId",
                "name",
                "description",
                "creationDate",
                "expiryDate",
                "sampleRegistry"
              ],
              "properties": {
                "ttlUrn": {
                  "type": "string",
                  "faker": "random.uuid"
                },
                "modelId": {
                  "type": "string",
                  "examples": [
                    "AFM_0000275"
                  ],
                  "pattern": "^AFM_[0-9]{7}$"
                },
                "name": {
                  "type": "string",
                  "examples": [
                    "SAM000001"
                  ],
                  "pattern": "^SAM[0-9]{7}$"
                },
                "description": {
                  "type": "string",
                  "examples": [
                    "Phosphoric acid (aq)"
                  ],
                  "faker": "commerce.productMaterial"
                },
                "creationDate": {
                  "type": "string",
                  "format": "date-time",
                  "faker": "customDate.last7"
                },
                "expiryDate": {
                  "type": "string",
                  "format": "date-time",
                  "faker": "customDate.next7"
                },
                "sampleRegistry": {
                  "type": "object",
                  "required": [
                    "ttlUrn"
                  ],
                  "properties": {
                    "ttlUrn": {
                      "type": "string",
                      "faker": "random.uuid"
                    }
                  }
                }
              }
            },
            "custodian": {
              "type": "object",
              "required": [
                "ttlUrn",
                "name"
              ],
              "properties": {
                "ttlUrn": {
                  "type": "string",
                  "faker": "random.uuid"
                },
                "name": {
                  "type": "string",
                  "examples": [
                    "jxd00005"
                  ],
                  "pattern": "^jxd[0-9]{5}$"
                }
              }
            }
          }
        }
      }
    },
    "plan": {
      "type": "object",
      "required": [
        "specification"
      ],
      "properties": {
        "specification": {
          "type": "object",
          "required": [
            "ttlUrn"
          ],
          "properties": {
            "ttlUrn": {
              "type": "string",
              "faker": "random.uuid"
            }
          }
        }
      }
    },
    "method": {
      "type": "object",
      "required": [
        "ttlUrn",
        "weighingMethodName",
        "assay"
      ],
      "properties": {
        "ttlUrn": {
          "type": "string",
          "faker": "random.uuid"
        },
        "weighingMethodName": {
          "type": "string",
          "examples": [
            "BWM000001"
          ],
          "pattern": "^BWM[0-9]{6}$"
        },
        "assay": {
          "type": "object",
          "required": [
            "ttlUrn",
            "name"
          ],
          "properties": {
            "ttlUrn": {
              "type": "string",
              "faker": "random.uuid"
            },
            "name": {
              "type": "string",
              "examples": [
                "ASM000001"
              ],
              "pattern": "^ASM[0-9]{6}$"
            }
          }
        }
      }
    },
    "assay": {
      "type": "object",
      "required": [
        "ttlUrn",
        "modelId",
        "name",
        "description",
        "registry"
      ],
      "properties": {
        "ttlUrn": {
          "type": "string",
          "faker": "random.uuid"
        },
        "modelId": {
          "type": "string",
          "examples": [
            "AFP_0002680"
          ],
          "pattern": "^AFP_[0-9]{7}$"
        },
        "name": {
          "type": "string",
          "examples": [
            "EXP000001"
          ],
          "pattern": "^EXP[0-9]{6}$"
        },
        "description": {
          "type": "object",
          "required": [
            "value",
            "modelId"
          ],
          "properties": {
            "value": {
              "type": "string",
              "examples": [
                "Standard for comparison in stability study"
              ],
              "faker": "company.catchPhrase"
            },
            "modelId": {
              "type": "string",
              "examples": [
                "AFRL_0000015"
              ],
              "pattern": "^AFRL_[0-9]{7}$"
            }
          }
        },
        "registry": {
          "type": "object",
          "title": "The Registry Schema",
          "required": [
            "ttlUrn"
          ],
          "properties": {
            "ttlUrn": {
              "type": "string",
              "faker": "random.uuid"
            }
          }
        }
      }
    },
    "batch": {
      "type": "object",
      "required": [
        "ttlUrn",
        "name",
        "modelId",
        "registry"
      ],
      "properties": {
        "ttlUrn": {
          "type": "string",
          "faker": "random.uuid"
        },
        "name": {
          "type": "string",
          "examples": [
            "BAT000001"
          ],
          "pattern": "^BAT[0-9]{6}$"
        },
        "modelId": {
          "type": "string",
          "examples": [
            "AFM_0000347"
          ],
          "pattern": "^AFM_[0-9]{7}$"
        },
        "registry": {
          "type": "object",
          "required": [
            "ttlUrn"
          ],
          "properties": {
            "ttlUrn": {
              "type": "string",
              "faker": "random.uuid"
            }
          }
        }
      }
    },
    "delivery": {
      "type": "object",
      "required": [
        "ttlUrn",
        "name",
        "registry"
      ],
      "properties": {
        "ttlUrn": {
          "type": "string",
          "faker": "random.uuid"
        },
        "name": {
          "type": "string",
          "examples": [
            "DEL000001"
          ],
          "pattern": "^DEL[0-9]{6}$"
        },
        "registry": {
          "type": "object",
          "required": [
            "ttlUrn"
          ],
          "properties": {
            "ttlUrn": {
              "type": "string",
              "faker": "random.uuid"
            }
          }
        }
      }
    },
    "personRegistry": {
      "type": "object",
      "required": [
        "ttlUrn"
      ],
      "properties": {
        "ttlUrn": {
          "type": "string",
          "faker": "random.uuid"
        }
      }
    }
  }
}

