{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "balance"
  ],
  "properties": {
    "balance": {
      "type": "object",
      "required": [
        "ttlUrn",
        "ttlUrn1",
        "modelId",
        "name",
        "site",
        "manufacturer",
        "precision",
        "accuracy",
        "range",
        "registry",
        "owner",
        "qualification",
        "calibration"
      ],
      "properties": {
        "ttlUrn": {
          "type": "string",
          "faker": "random.uuid"
        },
        "ttlUrn1": {
          "type": "string",
          "faker": "random.uuid"
        },
        "modelId": {
          "type": "string",
          "pattern": "^AFE_[0-9]{7}$"
        },
        "name": {
          "type": "string",
          "pattern": "^BAL[0-9]{3}$"
        },
        "site": {
          "type": "object",
          "required": [
            "location"
          ],
          "properties": {
            "location": {
              "type": "object",
              "required": [
                "ttlUrn",
                "modelId",
                "ttlUrn1",
                "modelId1",
                "siteName",
                "lab"
              ],
              "properties": {
                "ttlUrn": {
                  "type": "string",
                  "faker": "random.uuid"
                },
                "modelId": {
                  "type": "string",
                  "pattern": "^BFO_[0-9]{7}$"
                },
                "ttlUrn1": {
                  "type": "string",
                  "faker": "random.uuid"
                },
                "modelId1": {
                  "type": "string",
                  "pattern": "^AFSP_[0-9]{7}$"
                },
                "siteName": {
                  "type": "string",
                  "pattern": "^[A-Z]{3}$"
                },
                "lab": {
                  "type": "object",
                  "required": [
                    "name",
                    "modelId"
                  ],
                  "properties": {
                    "name": {
                      "type": "string",
                      "pattern": "^[0-9][A-Z][0-9]{3}$"
                    },
                    "modelId": {
                      "type": "string",
                      "examples": [
                        "AFR_0000919"
                      ],
                      "pattern": "^AFR_[0-9]{7}$"
                    }
                  }
                }
              }
            }
          }
        },
        "manufacturer": {
          "type": "object",
          "required": [
            "modelId",
            "name",
            "serialNumber",
            "manufactureDate",
            "model",
            "firmware"
          ],
          "properties": {
            "modelId": {
              "type": "string",
              "examples": [
                "AFR_0001258"
              ],
              "pattern": "^AFR_[0-9]{7}$"
            },
            "name": {
              "type": "string",
              "examples": [
                "Mettler Toledo"
              ],
              "faker": "company.companyName"
            },
            "serialNumber": {
              "type": "object",
              "required": [
                "modelId",
                "value"
              ],
              "properties": {
                "modelId": {
                  "type": "string",
                  "examples": [
                    "IAO_0000131"
                  ],
                  "pattern": "^IAO_[0-9]{7}$"
                },
                "value": {
                  "type": "string",
                  "examples": [
                    "ABC112049854"
                  ],
                  "pattern": "^ABC[0-9]{8}$"
                }
              }
            },
            "manufactureDate": {
              "type": "string",
              "format": "date-time",
              "faker": "dateISO.past"
            },
            "model": {
              "type": "object",
              "required": [
                "modelId",
                "value"
              ],
              "properties": {
                "modelId": {
                  "type": "string",
                  "examples": [
                    "IAO_0000017"
                  ],
                  "pattern": "^IAO_[0-9]{7}$"
                },
                "value": {
                  "type": "string",
                  "examples": [
                    "XSR104"
                  ],
                  "pattern": "^XSR[0-9]{3}$"
                }
              }
            },
            "firmware": {
              "type": "object",
              "required": [
                "modelId",
                "value"
              ],
              "properties": {
                "modelId": {
                  "type": "string",
                  "examples": [
                    "AFR_0001259"
                  ],
                  "pattern": "^AFR_[0-9]{7}$"
                },
                "value": {
                  "type": "string",
                  "examples": [
                    "1.3.5"
                  ],
                  "pattern": "^[1-3]\\.[0-9]\\.[0-9]$"
                }
              }
            }
          }
        },
        "precision": {
          "type": "object",
          "required": [
            "value",
            "unit"
          ],
          "properties": {
            "value": {
              "type": "number",
              "examples": [
                0.07
              ],
              "chance": {
                "floating": {
                  "min": 0.1,
                  "max": 1,
                  "fixed": 2
                }
              }
            },
            "unit": {
              "type": "string",
              "enum": [
                "Milligram"
              ]
            }
          }
        },
        "accuracy": {
          "type": "object",
          "required": [
            "value",
            "unit"
          ],
          "properties": {
            "value": {
              "type": "number",
              "examples": [
                0.07
              ],
              "chance": {
                "floating": {
                  "min": 0.1,
                  "max": 0.3,
                  "fixed": 2
                }
              }
            },
            "unit": {
              "type": "string",
              "enum": [
                "Milligram"
              ]
            }
          }
        },
        "range": {
          "type": "object",
          "required": [
            "modelId",
            "maximum",
            "minimum",
            "unit"
          ],
          "properties": {
            "modelId": {
              "type": "string",
              "examples": [
                "AFC_0000021"
              ],
              "pattern": "^AFC_[0-9]{7}$"
            },
            "maximum": {
              "type": "integer",
              "examples": [
                1200000
              ],
              "minimum": 1000000,
              "maximum": 5000000
            },
            "minimum": {
              "type": "number",
              "examples": [
                8.2
              ],
              "chance": {
                "floating": {
                  "min": 1,
                  "max": 10,
                  "fixed": 2
                }
              }
            },
            "unit": {
              "type": "string",
              "enum": [
                "Milligram"
              ]
            }
          }
        },
        "registry": {
          "type": "object",
          "required": [
            "ttlUrn"
          ],
          "properties": {
            "ttlUrn": {
              "type": "string",
              "faker": "random.uuid"
            }
          }
        },
        "owner": {
          "type": "object",
          "required": [
            "ttlUrn",
            "name"
          ],
          "properties": {
            "ttlUrn": {
              "type": "string",
              "faker": "random.uuid"
            },
            "name": {
              "type": "string",
              "examples": [
                "jxd00001"
              ],
              "pattern": "^jxd[0-9]{5}$"
            }
          }
        },
        "qualification": {
          "type": "object",
          "required": [
            "ttlUrn",
            "qualifier",
            "results",
            "installation",
            "operational",
            "performance",
            "endDatum"
          ],
          "properties": {
            "ttlUrn": {
              "type": "string",
              "faker": "random.uuid"
            },
            "qualifier": {
              "type": "object",
              "required": [
                "ttlUrn",
                "name"
              ],
              "properties": {
                "ttlUrn": {
                  "type": "string",
                  "faker": "random.uuid"
                },
                "name": {
                  "type": "string",
                  "examples": [
                    "jxd00002"
                  ],
                  "pattern": "^jxd[0-9]{5}$"
                }
              }
            },
            "results": {
              "type": "object",
              "required": [
                "ttlUrn"
              ],
              "properties": {
                "ttlUrn": {
                  "type": "string",
                  "faker": "random.uuid"
                }
              }
            },
            "installation": {
              "type": "object",
              "required": [
                "ttlUrn",
                "name"
              ],
              "properties": {
                "ttlUrn": {
                  "type": "string",
                  "faker": "random.uuid"
                },
                "name": {
                  "type": "string",
                  "examples": [
                    "IQ000001"
                  ],
                  "pattern": "^IQ[0-9]{6}$"
                }
              }
            },
            "operational": {
              "type": "object",
              "required": [
                "ttlUrn",
                "name"
              ],
              "properties": {
                "ttlUrn": {
                  "type": "string",
                  "faker": "random.uuid"
                },
                "name": {
                  "type": "string",
                  "examples": [
                    "OQ000001"
                  ],
                  "pattern": "^OQ[0-9]{6}$"
                }
              }
            },
            "performance": {
              "type": "object",
              "required": [
                "ttlUrn",
                "name"
              ],
              "properties": {
                "ttlUrn": {
                  "type": "string",
                  "faker": "random.uuid"
                },
                "name": {
                  "type": "string",
                  "examples": [
                    "PQ000001"
                  ],
                  "pattern": "^PQ[0-9]{6}$"
                }
              }
            },
            "endDatum": {
              "type": "string",
              "faker": "customDate.last7"
            }
          }
        },
        "calibration": {
          "type": "object",
          "required": [
            "ttlUrn",
            "endDatum",
            "calibrator",
            "results"
          ],
          "properties": {
            "ttlUrn": {
              "type": "string",
              "faker": "random.uuid"
            },
            "endDatum": {
              "type": "string",
              "format": "date-time",
              "faker": "customDate.last7"
            },
            "calibrator": {
              "type": "object",
              "required": [
                "ttlUrn",
                "name"
              ],
              "properties": {
                "ttlUrn": {
                  "type": "string",
                  "faker": "random.uuid"
                },
                "name": {
                  "type": "string",
                  "examples": [
                    "jxd00003"
                  ],
                  "pattern": "^jxd[0-9]{5}$"
                }
              }
            },
            "results": {
              "type": "object",
              "required": [
                "ttlUrn",
                "expiryDate",
                "range",
                "precision",
                "accuracy"
              ],
              "properties": {
                "ttlUrn": {
                  "type": "string",
                  "faker": "random.uuid"
                },
                "expiryDate": {
                  "type": "string",
                  "format": "date-time",
                  "faker": "customDate.next7"
                },
                "range": {
                  "type": "object",
                  "required": [
                    "maximum",
                    "minimum",
                    "unit"
                  ],
                  "properties": {
                    "maximum": {
                      "type": "integer",
                      "examples": [
                        1000000
                      ],
                      "minimum": 1000000,
                      "maximum": 5000000
                    },
                    "minimum": {
                      "type": "integer",
                      "examples": [
                        82
                      ],
                      "minimum": 10,
                      "maximum": 100
                    },
                    "unit": {
                      "type": "string",
                      "enum": [
                        "Milligram"
                      ]
                    }
                  }
                },
                "precision": {
                  "type": "object",
                  "required": [
                    "value",
                    "unit"
                  ],
                  "properties": {
                    "value": {
                      "type": "number",
                      "chance": {
                        "floating": {
                          "min": 0.1,
                          "max": 0.5,
                          "fixed": 2
                        }
                      }
                    },
                    "unit": {
                      "type": "string",
                      "enum": [
                        "Milligram"
                      ]
                    }
                  }
                },
                "accuracy": {
                  "type": "object",
                  "required": [
                    "value",
                    "unit"
                  ],
                  "properties": {
                    "value": {
                      "type": "number",
                      "chance": {
                        "floating": {
                          "min": 0.1,
                          "max": 1,
                          "fixed": 2
                        }
                      }
                    },
                    "unit": {
                      "type": "string",
                      "enum": [
                        "Milligram"
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
