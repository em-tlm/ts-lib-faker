{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "project"
  ],
  "properties": {
    "project": {
      "type": "object",
      "required": [
        "ttlUrn",
        "name",
        "registry"
      ],
      "properties": {
        "ttlUrn": {
          "type": "string",
          "faker": "random.uuid"
        },
        "name": {
          "type": "string",
          "examples": [
            "PROJ00001"
          ],
          "pattern": "^PROJ[0-9]{5}$"
        },
        "registry": {
          "type": "object",
          "required": [
            "ttlUrn"
          ],
          "properties": {
            "ttlUrn": {
              "type": "string",
              "faker": "random.uuid"
            }
          }
        }
      }
    }
  }
}
