{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "ttlUrn",
    "name"
  ],
  "properties": {
    "ttlUrn": {
      "type": "string",
      "faker": "random.uuid"
    },
    "name": {
      "type": "string",
      "examples": [
        "jxd00001"
      ],
      "pattern": "^jxd[0-9]{5}$"
    }
  }
}
